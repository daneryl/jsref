{
  "name": "jsref_dev",
  "description": "JavaScript refactoring language server (dev)",
  "version": "1.0.7",
  "publisher": "slonoed",
  "author": "Dmitry Manannikov <email@slonoed.net> (https://slonoed.net/)",
  "license": "WTFPL",
  "repository": "https://github.com/slonoed/jsref",
  "main": "./extension.js",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "commands": [
      {
        "command": "jsrefdev.restart",
        "title": "Restart jsref lang server"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "dependencies": {
    "vscode-languageclient": "^5.3.0-next.3"
  },
  "engines": {
    "vscode": "^1.33.0"
  }
}
